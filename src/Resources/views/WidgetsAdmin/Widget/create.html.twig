{% extends '@SamuelmcFdnContentWidgets/Shared/page.html.twig' %}

{% block content %}

    <h1>Create new Widget\Widget</h1>

    {{ form_start(form) }}

        {{ form_widget(form) }}

        <button type="submit" class="button">
